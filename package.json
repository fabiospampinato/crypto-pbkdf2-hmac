{
  "name": "crypto-pbkdf2-hmac",
  "description": "Isomorphic wrapper for the PBKDF2-HMAC key derivation function.",
  "version": "1.0.0",
  "main": "dist/node.js",
  "types": "dist/node.d.ts",
  "exports": {
    "node": "./dist/node.js",
    "default": "./dist/browser.js"
  },
  "scripts": {
    "benchmark": "node tasks/benchmark.js",
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "fava",
    "test:watch": "fava -w",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/crypto-pbkdf2-hmac/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/crypto-pbkdf2-hmac.git"
  },
  "keywords": [
    "crypto",
    "pbkdf2-hmac",
    "pbkdf2",
    "key",
    "derivation",
    "isomorphic"
  ],
  "dependencies": {
    "uint8-to-hex": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "fava": "^0.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "typescript-transform-export-interop": "^1.0.4"
  }
}
